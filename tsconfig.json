{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@mui/*": ["node_modules/@mui/*"]
    },
    "target": "es5",
    "lib": ["dom", "ES2023"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": ["src", "src/components/globalComponents/dataDisplay/teacherInfractionPeriodBarChart.js"]
}